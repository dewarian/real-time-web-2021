{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["google"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "space-before-function-paren": ["error", "always"],
    "no-tabs": "error",
    "indent": ["error", 2]
  },
  "ignorePatterns": ["**/node_modules", "**/package.json"]
}